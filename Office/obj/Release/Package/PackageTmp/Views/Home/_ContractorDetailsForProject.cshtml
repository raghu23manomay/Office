@model IEnumerable<office.Models.ContractorDetails>

@{
    Layout = null;
}
<script src="~/js/jquery-2.1.1.js"></script>

<table class="table" id="contractorlist">
    <thead>

    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr><td class="hidden multicontractor" id="@item.ContractorTypeId">@item.ConstractorId</td><td>@item.Name</td><td><span style="color: darkgray;font-size: smaller;">@item.ContractorType</span></td><td><i class="fa fa-times deleterow pull-right" id="mydel" aria-hidden="true"></i></td></tr>
        }

    </tbody>
</table>

