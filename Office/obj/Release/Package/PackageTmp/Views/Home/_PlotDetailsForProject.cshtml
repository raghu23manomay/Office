@model IEnumerable<office.Models.PlotDetails>

@{
    Layout = null;
}
<script src="~/js/jquery-2.1.1.js"></script>


<table class="table" id="Plotlist">
    <thead>
        <tr>
            <th>Plot No</th>
            <th>Area</th>
            <th>Unit</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr><td>@item.PlotNo</td><td>@item.Area</td><td class="hidden">@item.UnitID</td><td>@item.Unit</td><td><i class="fa fa-times deleterow pull-right" id="mydel" aria-hidden="true"></i></td></tr>
        }
    </tbody>

</table>

